@model ASPMVCIdentityDemo.Models.LoginModel
@{
    ViewBag.Title = "Login";
}

<h2>Login View</h2>


@Html.ValidationSummary(true, "Please check the following error and try again!", new { @class="text-danger"})

@using (Html.BeginForm())
{
    @Html.EditorForModel()
    <p>
        <button type="submit">Log In</button>
    </p>
}
<a href="@Html.Raw(ViewBag.back)">Return URL :@ViewBag.back</a>
